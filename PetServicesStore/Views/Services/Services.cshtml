<!-- breadcrumbs -->
<section class="w3l-inner-banner-main">
    <div class="about-inner editContent">
        <div class="container">
            <ul class="breadcrumbs-custom-path">
                <li class="right-side propClone"><a href="@Url.Action("Index","Default")" class="editContent">Home <span class="fa fa-angle-right" aria-hidden="true"></span></a> <p></li>
                <li class="active editContent">Services</li>
            </ul>
        </div>
    </div>
</section>
<!-- breadcrumbs //-->


<!--service price table-->
@{ 
    DAO ob = new DAO();
    ob.getData();
}
<section class="container-fluid outer-price-table">
    <div class="row align-items-center justify-content-center price-board">
        <table class="table col-3 border">
            <thead>
                <tr>
                    <td scope="col" class="btn-primary">Services Name</td>
                    <td scope="col" class="btn-primary">Price</td>
                </tr>
            </thead>
            <tbody>
                @foreach (services item in ob.ServicesLst.Values)
                {
                    <tr class="text-dark price-table-bg">
                        <td>@item.name</td>
                        <td>$@item.price</td>
                    </tr>
                }
            </tbody>
        </table>
        <button type="button" class="btn btn-primary justify-content-end btnBookAppointment"><a href="@Url.Action("Index","Default")">Book Now!</a></button>
    </div>
</section>
<!--service price table end-->

